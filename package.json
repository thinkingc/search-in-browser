{
  "name": "search-in-browser",
  "displayName": "Search In Browser",
  "description": "It can help you search selection or input text in default browser.",
  "version": "0.1.0",
  "publisher": "chenwenhao",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "search",
    "browser",
    "google",
    "baidu",
    "search in browser"
  ],
  "activationEvents": [
    "onCommand:easy.search"
  ],
  "icon": "images/logo.png",
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "easy.search",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "easy.search",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "easy.search",
        "title": "Search In Browser",
        "category": "Search",
        "icon": {
          "dark": "./images/search-icon.png",
          "light": "./images/search-icon.png"
        }
      }
    ],
    "keybindings": [
      {
        "command": "easy.search",
        "key": "ctrl+g ctrl+g",
        "mac": "cmd+g cmd+g"
      }
    ],
    "configuration": {
      "title": "Google Search Config",
      "type": "object",
      "properties": {
        "search.QueryTemplate": {
          "type": "string",
          "default": "https://www.google.com/search?q=%SELECTION%",
          "description": "Search Query"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.31.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  },
  "bugs": {
    "url": "https://github.com/thinkingc/vscode-search-in-browser/issues",
    "email": "2407606301@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkingc/vscode-search-in-browser.git"
  },
  "homepage": "https://github.com/thinkingc/vscode-search-in-browser/blob/main/README.md"
}
